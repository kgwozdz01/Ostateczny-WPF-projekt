        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ostateczny_WPF_projekt"
        mc:Ignorable="d"
        Title="TerminyUmowioneOkno" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="GridTerminyUmowione" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button x:Name="Odswiezzz" Content="Odśwież, by zobaczyć dane tabeli" HorizontalAlignment="Left" Margin="42,203,0,0" VerticalAlignment="Top" Height="35" Width="220" Click="Odswiezzz_Click"/>
        <Label x:Name="note" Content="note: dane pojawiaja sie w tabeli automatycznie" HorizontalAlignment="Left" Margin="53,161,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
